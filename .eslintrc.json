{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "globalReturn": true,
            "jsx": false
        }
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "jasmine": true
    },
    "plugins": [

    ],
    "globals": {
        "document": false,
        "navigator": false,
        "window": false
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [2, 4, {"SwitchCase": 1}],
        "curly": [2, "multi-line", "consistent"],
        "one-var": 0,
        "no-unused-vars": 0,
        "object-curly-spacing": 0,
        "operator-linebreak": 0,
        "padded-blocks": 0,
        "no-negated-condition": 0,
        "quote-props": 0,
        "capitalized-comments": 0,
        "no-prototype-builtins": 0,
        "no-multi-assign": 0,
        "no-multi-spaces": 0,
        "no-lonely-if": 0,
        "semi": [2, "always"],
        "valid-jsdoc": 0,
        "space-before-function-paren": [2, "never"]
    }
}
